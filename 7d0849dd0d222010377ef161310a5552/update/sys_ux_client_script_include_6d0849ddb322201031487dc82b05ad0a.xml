<?xml version="1.0" encoding="UTF-8"?><record_update table="sys_ux_client_script_include">
    <sys_ux_client_script_include action="INSERT_OR_UPDATE">
        <access>public</access>
        <api_name>x_snc_lastship.userPreferenceUtils</api_name>
        <includes/>
        <name>userPreferenceUtils</name>
        <required_translations/>
        <script><![CDATA[function include(...args) {
    const userPrefs = args[1];

    /**
     * getUserPreference method returns user preference value.
     *
     * @param name
     * @return {boolean}
     */
    const getUserPreference = (name) => {
        return userPrefs[name] === 'true' || userPrefs[name] === true;
    };

    return ({
        getUserPreference: getUserPreference
    });
}]]></script>
